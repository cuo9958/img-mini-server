{
    "rules": {
        "valid-jsdoc": 0,
        "strict": 0,
        "indent": [
            "error",
            4
        ],
        "quotes": 0,
        "comma-dangle": 0,
        "spaced-comment": 0,
        "no-trailing-spaces": 0,
        "eol-last": 0,
        "semi": 0,
        "arrow-parens": 0,
        "space-before-function-paren": 0,
        "object-shorthand": 0,
        "prefer-const": 0,
        "no-return-assign": 0,
        "array-bracket-spacing": 0,
        "linebreak-style": 0
    }
}